<h1> Eat-Da-Burger!</h1>

<ul>
  {{#each burger}}
      <li>
        {{this.burger_name}}

      </li>
  {{/each}}
</ul>



<form action="/" method="POST">

<p>Enter the burger you would like to eat and click submit button!</p>
    <input type="text" name="name">
    <button type="submit">Submit</button>
</form>
